<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <DatabaseNamePrefix>$(BuildAgentName)_</DatabaseNamePrefix>

    <DataSourceMsSql2005>localhost\MsSql2005</DataSourceMsSql2005>
    <DataSourceMsSql2008>localhost\MsSql2008</DataSourceMsSql2008>
    <DataSourceMsSql2008R2>$(MSSQLHOST)\MsSql2008R2</DataSourceMsSql2008R2>
    <DataSourceMsSql2012>$(MSSQLHOST)\MsSql2012</DataSourceMsSql2012>
    <DataSourceMsSql2014>$(MSSQLHOST)\MsSql2014</DataSourceMsSql2014>

    <DatabaseDirectoryMsSql2005>C:\Databases\MsSql2005</DatabaseDirectoryMsSql2005>
    <DatabaseDirectoryMsSql2008>C:\Databases\MsSql2008</DatabaseDirectoryMsSql2008>
    <DatabaseDirectoryMsSql2008R2>$(MSSQL2008R2_DATADIR)</DatabaseDirectoryMsSql2008R2>
    <DatabaseDirectoryMsSql2012>$(MSSQL2012_DATADIR)</DatabaseDirectoryMsSql2012>
    <DatabaseDirectoryMsSql2014>$(MSSQL2014_DATADIR)</DatabaseDirectoryMsSql2014>
  </PropertyGroup>
</Project>